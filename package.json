{
  "name": "bal-zksync-shit",
  "scripts": {
    "start": "npx hardhat node",
    "build": "npx hardhat compile",
    "build:zksync": "rimraf ./artifacts-zk && rimraf ./cache-zk && npx hardhat compile --network zkSyncLocal",
    "test": "npx hardhat test",
    "forge:test": "forge test -vvv",
    "forge:test:watch": "forge test --watch --run-all -vvv",
    "deploy": "npx hardhat run ./scripts/deploy.ts",
    "deploy:zksync:testnet": "npx hardhat --network zkSyncTestnet deploy-zksync"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
